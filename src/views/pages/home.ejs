        <script>
          // Counter function
          // Counter function
  function countUp(elementId, start, end, duration, isPercentage = false) {
    const element = document.getElementById(elementId);
    let current = start;
    const increment = (end - start) / (duration / 50); // Increment based on frame rate

    function updateCounter() {
      current += increment;
      if (current >= end) {
        current = end;
        clearInterval(interval);
      }
      element.textContent = Math.floor(current) + (isPercentage ? "%" : ""); // Add '%' if needed
    }

    const interval = setInterval(updateCounter, 50); // Update every 50ms (20fps)
  }

  // Trigger the count-up on page load
  window.onload = function() {
    countUp("clients", 0, 150, 5000);     // 0 to 500 clients in 2 seconds
    countUp("years", 0, 4, 5000);        // 0 to 10 years in 2 seconds
    countUp("projects", 0, 150, 5000);    // 0 to 150 projects in 2 seconds
    countUp("outcomes", 0, 100, 5000, true); // 0 to 100% outcomes in 2 seconds (with '%')
  };
