          <div class="hidden w-full md:block md:w-auto" id="mobile-menu">
            <ul class="flex flex-col text-[18px] mt-5 md:flex-row md:h-6  md:space-x-8 md:mt-0 ">
              <li>
                <a href="/services" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent md:border-b-0  border-b-2 md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2 md:hover:border-red-300"> Our Services</a>
              </li>
              <li>
                <a href="/codes" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent  border-b-2 md:border-b-0 md:border-transparent md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2  md:hover:border-red-300">CDR ANZSCO code</a>
              </li>
              <li>
                <a href="/pricing" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent border-b-2 md:border-b-0 md:border-transparent  md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2 md:hover:border-red-300">Pricing</a>
              </li>
              <li>
                <a href="/blog" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent  border-b-2 md:border-b-0 md:border-transparent  md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2 md:hover:border-red-300"> Blog </a>
              </li>
              <li>
                <a href="/about" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent border-b-2 md:border-b-0 md:border-transparent  md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2 md:hover:border-red-300"> About us </a>
              </li>
              <li>
                <a href="/contact" class="block py-2 pr-4 pl-3 text-gray-500 rounded hover:bg-red-200 md:hover:bg-transparent border-b-2 md:border-b-0 md:border-transparent  md:hover:text-red-500 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent transition-all duration-500 md:hover:border-b-2 md:hover:border-red-300">Contact us</a>
              </li>
              <button class="md:mt-0 mt-4 md:hidden bg-red-500 rounded-md  text-white px-4 py-2.5" >
                Register for Service
            </button>
            </ul>
          </div>
            <button class=" hidden md:block bg-red-500 rounded-md  text-white px-4 py-2.5" >
                Register for Service
            </button>
        </div>
      </nav>
</body>
<script>
    document.getElementById('menu-toggle').addEventListener('click', function() {
      const menu = document.getElementById('mobile-menu');
      const menuIcon = document.getElementById('menu-icon');
  
      menu.classList.toggle('hidden');
      
      // Toggle between Hamburger and Close icons
      if (menuIcon.classList.contains('fa-bars')) {
        menuIcon.classList.remove('fa-bars');
        menuIcon.classList.add('fa-times');
      } else {
        menuIcon.classList.remove('fa-times');
        menuIcon.classList.add('fa-bars');
      }
    });
  </script>
</html>


